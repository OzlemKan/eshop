@using eshop.Data.ViewComponents
@using eshop.Data.Cart;
@using Microsoft.AspNetCore.Mvc;

<!DOCTYPE html>
<html lang="en">
<head style="background-color: #eee4df">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] <img src="Image/Logo_Isis.png" /> - Jewelry eshop</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/eshop.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="css/carousel.css" asp-append-version="true" />
    <link rel="stylesheet" href="css/navbars.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.1/font/bootstrap-icons.css">
</head>
<body style="background-color: #eee4df">
    <header>
        <partial name="_navbar"/>
        
    </header>
<div class="container-fluid">
    
     


    <main role="main" class="pb-3">
        @RenderBody()

    </main>
</div>


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>



